SELECT DISTINCT Дата
FROM наклзаголовки
WHERE `Код подразделения` = "0429"



SELECT a.Номер, b.НаимКАгента, a.ТипНакладной
FROM наклзаголовки as a, контрагенты as b
where a.КодКАгента = b.КодКАгента AND a.`Код подразделения`= "0429" and DATE(a.Дата) = "2002-02-11"
order by a.Номер


SELECT КодПодразделения, КодТовара  FROM запасы
  UNION 
SELECT КодПодразделения, КодТовара FROM запасы_copy


INSERT INTO товары (кодтовара, наимтовара, цена1, цена2)
values ('100000000002', 'Косолапый', 11, 14)


update товары SET наимтовара='Косолапый Миха' where КодТовара='100000000002'

delete from товары where КодТовара='100000000002'

delete from товары where Цена1 <= 14
